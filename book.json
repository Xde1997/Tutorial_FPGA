{
  "title": "FPGA原理与AI加速应用教程",
  "description": "面向软件工程师的FPGA实战指南",
  "author": "FPGA教程团队",
  "language": "zh-hans",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "expandable-chapters",
    "search",
    "copy-code-button",
    "theme-default",
    "back-to-top-button",
    "github",
    "splitter",
    "toc"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/Xde1997/Tutorial_FPGA"
    },
    "theme-default": {
      "showLevel": true
    },
    "toc": {
      "addClass": true,
      "className": "toc"
    },
    "search": {
      "maxIndexSize": 100000,
      "ignoreCase": true
    },
    "expandable-chapters": {},
    "copy-code-button": {
      "copyText": "复制",
      "format": "html"
    }
  },
  "variables": {
    "themeColor": "#3f51b5",
    "themeColorSecondary": "#ff4081"
  }
} 